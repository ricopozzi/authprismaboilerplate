// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../dev.db"
}

model User {
  id String @id @default(uuid())
  email String @unique
  password String
  name String
  created_at DateTime @default(now())

  @@map("users")
}

model Mob {
  id String @id @default(uuid())
  adress String
  rooms String
  suite String  @default("0")
  bathrooms String
  maxpeople String
  created_at DateTime @default(now())

  @@map("mobs")
}
